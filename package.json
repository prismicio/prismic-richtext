{
  "name": "prismic-richtext-jsx",
  "version": "0.0.0-development",
  "description": "Convert Rich Text raw data to generic tree to simplify serialization, with ability to pass down parent node props to child nodes",
  "main": "dist/prismic-richtext.min.js",
  "typings": "typings/index",
  "scripts": {
    "prebuild": "tsc",
    "build": "webpack --debug; webpack -p",
    "dev": "webpack -d --watch",
    "docs": "typedoc --mode file --out ./docs/ ./src/",
    "lint": "tslint src",
    "posttest": "tslint lib",
    "test": "nyc --reporter=html --reporter=text mocha  -r jsdom-global/register",
    "semantic-release": "semantic-release",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/harrison-ifeanyichukwu/prismic-richtext-jsx.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.1.0",
    "@types/node": "^12.7.5",
    "@types/ramda": "0.25.44",
    "babel-loader": "^8.0.4",
    "babel-plugin-ramda": "^2.0.0",
    "chai": "^4.1.2",
    "chai-subset": "^1.6.0",
    "jsdom": "^13.1.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^4.0.1",
    "nyc": "14.1.1",
    "semantic-release-cli": "5.2.1",
    "tslint": "^5.11.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.2.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "yargs": "^10.0.3",
    "semantic-release": "^15.13.24"
  }
}
